<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <plugin name="scan" filename="libscan.so"/>
    <plugin name="lighthouse" filename="liblighthouse.so"/>
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- A velodyne lidar -->
    <include>
      <uri>model://velodyne</uri>
    </include>

  </world>
</sdf>
